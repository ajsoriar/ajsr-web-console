{
  "name": "purple-console",
  "version": "1.0.1",
  "description": "Javascript component that prints logs in a DIV layer. It can be useful when dealing with special hardware like Smart TVs or Set-Top Boxes which do not allow debugging.",
  "main": "dist/purple-consoli.min.js",
  "repository": "https://github.com/ajsoriar/purple-console.git",
  "author": "asr <ajsoriar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "grunt": "~0.4",
    "grunt-cleanempty": "^1.0.3",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-uglify": "~0.2.2"
  },
  "scripts": {
    "start-watch": "nodemon run-babel index.js",
    "wp-server": "webpack-dev-server",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run wp-server\"",
    "build": "grunt build"
  }
  
}
